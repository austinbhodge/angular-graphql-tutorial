{
  "name": "angular-graphql-tutorial",
  "version": "1.0.0",
  "description": "Learn how to make isomorphic apps with angular and graphql",
  "main": "dist/main.js",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:notify": "npm run test:watch -- --notify",
    "coverage": "npm test -- --coverage",
    "coverage:notify": "npm run coverage -- --watch --notify",
    "start": "npm run build && node dist/main.js",
    "start:watch": "concurrently \"npm run build:watch\" \"node-dev dist/main.js\"",
    "preupver": "npm test",
    "upver": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/austinbhodge/angular-graphql-tutorial.git"
  },
  "author": "Austin Hodge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/austinbhodge/angular-graphql-tutorial/issues"
  },
  "homepage": "https://github.com/austinbhodge/angular-graphql-tutorial#readme",
  "keywords": [
    "apollo",
    "backend",
    "apollo-server",
    "typescript",
    "webpack",
    "graphql",
    "graphiql",
    "express"
  ],
  "dependencies": {
    "@types/body-parser": "1.16.3",
    "@types/express": "^4.0.35",
    "body-parser": "^1.17.2",
    "express": "^4.15.2",
    "graphql": "0.9.6",
    "graphql-server-express": "^0.7.2",
    "graphql-tools": "0.11.0",
    "mongodb": "^2.2.27"
  },
  "devDependencies": {
    "@types/graphql": "^0.9.1",
    "@types/jest": "^19.2.3",
    "@types/mongodb": "^2.2.2",
    "@types/node": "^7.0.18",
    "awesome-typescript-loader": "^3.1.3",
    "concurrently": "^3.4.0",
    "jest": "^20.0.1",
    "jest-cli": "^20.0.1",
    "node-dev": "^3.1.3",
    "standard-version": "^4.0.0",
    "ts-jest": "^20.0.3",
    "tslint": "^5.2.0",
    "tslint-loader": "^3.5.3",
    "typescript": "2.3.2",
    "webpack": "^2.5.1",
    "webpack-node-externals": "^1.6.0"
  }
}
